{
	"name": "backend",
	"main": "./index.js",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "npx nodemon --watch src --watch ../../built/_vite_ -e 'js,ts,json' --exec 'npm run build && npm run start'",
    "dev:frontend-only": "npx nodemon --watch src --watch ../../built/_vite_ -e 'js,ts,json' --exec 'npm run start'",
		"livereload": "npx livereload ./ -e 'start-time'",
		"build:start": "npm run build && npm run start",
		"start": "node --experimental-json-modules --experimental-specifier-resolution=node ./built/boot/index.js",
		"clean": "rm -rf built",
		"watch:swc": "swc src -d built -D -w",
		"migrate": "typeorm migration:run -d ormconfig.js",
		"build:swc": "swc src --out-dir built --source-maps && rsync -av --exclude='server/api' --exclude='models' built/src/ built/ && rm -rf built/src",
		"build:swc:origin": "swc src -d built -D && rsync -av built/src/ built/ && rm -rf built/src",
		"build": "tsc -p tsconfig.json || echo done. && tsc-alias -p tsconfig.json",
		"start:test": "NODE_ENV=test node ./built/index.js",
		"watch": "node watch.mjs",
		"type-check": "tsc --noEmit --project tsconfig.json",
		"type-check:watch": "tsc --noEmit --watch",
		"npm:update-lock": "npm install --package-lock-only",
		"lint": "tsc --noEmit && eslint --quiet \"src/**/*.ts\"",
		"mocha": "cross-env NODE_ENV=test TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=\"./test/tsconfig.json\" mocha",
		"test": "npm run jest",
		"db:migrate": "npx typeorm migration:run -d ormconfig.js",
		"db:revert": "npx typeorm migration:revert -d ormconfig.js",
		"db:migration:generate": "read NAME; npm run build && npx typeorm migration:generate -d ormconfig.js migration/$NAME",
		"upgrade-interactive": "npx npm-check --update",
		"jest-and-coverage": "cross-env NODE_ENV=test node --experimental-vm-modules --experimental-import-meta-resolve node_modules/jest/bin/jest.js --coverage --forceExit --runInBand --detectOpenHandles",
		"jest": "cross-env NODE_ENV=test node --experimental-vm-modules --experimental-import-meta-resolve node_modules/jest/bin/jest.js --forceExit --runInBand --detectOpenHandles",
		"check:connect": "node ./check_connect.js",
		"build:tsc": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
		"typecheck": "tsc --noEmit",
		"eslint": "eslint --quiet \"src/**/*.ts\"",
		"jest-clear": "cross-env NODE_ENV=test node --experimental-vm-modules --experimental-import-meta-resolve node_modules/jest/bin/jest.js --clearCache",
		"test-and-coverage": "npm run jest-and-coverage"
	},
